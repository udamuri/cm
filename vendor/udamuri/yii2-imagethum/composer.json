{
    "name": "udamuri/yii2-imagethum",
    "description": "Yii2 imagethum",
    "type": "yii2-extension",
    "homepage": "https://github.com/udamuri/yii2-imagethum.git",
    "keywords": ["yii2","extension","image","udamuri","thumnail"],
    "license": "MIT",
    "authors": [
        {
            "name": "Muri Budiman",
            "email": "udamuri@gmail.com",
            "homepage": "https://github.com/udamuri/"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url":  "https://github.com/udamuri/yii2-imagethum.git"
        }
    ],
    "require": {
        "udamuri/yii2-imagethum": "1.0"
    },
    "autoload": {
        "psr-4": {
            "udamuri\\imagethum\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
